
<h1>Chefs | <a href="Dish">Dishes</a></h1>
@model List<Chef>

<h3>Check out our roaster of Chefs</h3>


  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Age</th>
        <th scope="col"># Of Dishes</th>
      </tr>
    </thead>
    <tbody>
      @foreach (var item in ViewBag.AllChefs)
      {
        <tr>
          <td>@item.FirstName @item.LastName</td>
          <td>@item.DateofBirth</td>
          <td>@item.CreatedDishes.Count</td>
        </tr>
      }
    </tbody>
  </table>
  <a href="/AddChef">Add Chef</a>



